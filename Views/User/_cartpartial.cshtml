@model IEnumerable<onlineshop.Models.Products>
<h3>@ViewBag.data</h3>
@{ foreach(Products p in Model)
    {

<ol class="items">
    <li>
        <a href="shop-single-product-v1.html" class="product-image">
            <img src="@p.ProductImg1" alt="Sample Product ">
        </a>
        <div class="product-details">
            <div class="close-icon">
                <a href="javascript:void(0);"><i class="fa fa-close"></i></a>
            </div>
            <p class="product-name">
                <a href="shop-single-product-v1.html">@p.ProductTitle</a>
            </p>
            <strong class="x">1</strong> x <span class="price text-primary">@p.ProductPrice</span>
        </div><!-- end product-details -->
    </li><!-- end item -->

</ol>
    }
}